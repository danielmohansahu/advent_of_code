#!/usr/bin/sbcl --script
; Figuring out starting packets in datastreams

; main input
(defvar input "")

; determine whether or not the given string (size SIZE) is the start string
(defun is-start (substr)
  (= (length substr) (length (remove-duplicates substr)))
)

; function to test a given input and return the index of the start sequence
(defun parse-start-idx (str size)
  ; iterate through the string, starting from 
  (let ((res -1))
    ; iterate from SIZE to end of string
    (dotimes (i (- (length str) size))
      ; check the subsequence
      (let ((start i) (end (+ i size)))
        (if (is-start (subseq str start end))
          (progn
            (format t "Start bit ~A (idx ~A) in ~A~%" (subseq str start end) end str)
            (return :early-exit!)
          )
        )
      )
    )
  )
)

; call for test inputs - part A needs 4 bits
(parse-start-idx "mjqjpqmgbljsphdztnvjfqwrcgsmlb" 4)
(parse-start-idx "bvwbjplbgvbhsrlpgdmjqwftvncz" 4)
(parse-start-idx "nppdvjthqldpwncqszvftbrmjlhg" 4)
(parse-start-idx "nznrnfrfntjfmvfwmzdfjlvtqnbhcprsg" 4)
(parse-start-idx "zcfzfwzzqfrljwzlrfnpqdbhtmscgvjw" 4)

; call for real input
(parse-start-idx input 4)

; call for test inputs - part B needs 14 bits
(parse-start-idx "mjqjpqmgbljsphdztnvjfqwrcgsmlb" 14)
(parse-start-idx "bvwbjplbgvbhsrlpgdmjqwftvncz" 14)
(parse-start-idx "nppdvjthqldpwncqszvftbrmjlhg" 14)
(parse-start-idx "nznrnfrfntjfmvfwmzdfjlvtqnbhcprsg" 14)
(parse-start-idx "zcfzfwzzqfrljwzlrfnpqdbhtmscgvjw" 14)

; call for real input
(parse-start-idx input 14)
